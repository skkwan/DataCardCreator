#!/bin/sh

preselection='isoTight_1>0.5&&isoTight_2>0.5&&'\
'abs(eta_1)<2.1&&abs(eta_2)<2.1&&'\
'againstMuonTight3_1>0&&againstMuonTight3_2>0&&'\
'againstElectronVLooseMVA6_1>0&&againstElectronVLooseMVA6_2>0&&'\
'npv>0&&diLeptons==0&&extraelec_veto==0&&extramuon_veto==0'

luminosity=35870 #pb
outputDir='diTauPlots'
mkdir $outputDir

folder='tt_inclusive'
direc='/hdfs/store/user/ojalvo/ztt_weighted_Apr7_svFit/vbfH125.root'

MakeDataCardHThTh_2016 channel='diTau'  folder=$folder  preselection=$preselectiondir=$direc weight='__WEIGHT__*GENWEIGHT'  luminosity=$luminosity  variable='m_sv' bins=30 min=0 max=300 luminosity=$luminosity bins=30 min=2 max=302 outputFile=$outputDir'/tauTau_m_sv.root'